    "prepublish": "tsc"
  },
  "dependencies": {
    "bloc-them": "^4.0.0",
    "lit-html": "^1.1.2"
  },
  "devDependencies": {
    "@types/node": "13.11.1",
    "@web/dev-server": "^0.0.12",
    "typescript": "~4.0.3",
    "concurrently": "^5.1.0",
    "tslib": "^1.11.0"
  }
}
