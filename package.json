{
  "name": "route-them",
  "version": "6.0.0",
  "description": "Routing webcomponents to use with lit-bloc-js library",
  "author": "route-them",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/anuragvohraec/route-them.git"
  },
  "scripts": {
    "start": "concurrently --kill-others --names tsc,web-dev-server \"npm run tsc:watch\" \"web-dev-server --app-index demo/index.html --node-resolve --open --watch\"",
